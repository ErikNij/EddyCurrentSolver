FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      VRe;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    axis
    {
        type            zeroGradient;
    }
    region_conductor_to_region_space
    {
        type            fixedGradient;
        gradient        uniform 0;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}


