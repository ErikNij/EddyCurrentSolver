FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      fluidIndicator;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    axis
    {
        type            slip;
    }
    trackedSurface
    {
        type            zeroGradient;
    }
    fixedWallBottom
    {
        type            zeroGradient;
    }
    fixedWallSide
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}


