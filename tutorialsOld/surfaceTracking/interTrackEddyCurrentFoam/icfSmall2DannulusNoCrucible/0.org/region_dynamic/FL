FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      FL;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    axis
    {
        type            calculated;
        value           $internalField;
    }
    trackedSurface
    {
        type            calculated;
        value           $internalField;
    }
    fixedWallSide
    {
        type            calculated;
        value           $internalField;
    }
    fixedMesh
    {
        type            calculated;
        value           $internalField;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}


// ************************************************************************* //
