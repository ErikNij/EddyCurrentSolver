FoamFile
{
    version     2.0;
    format      ascii;
    class       edgeScalarField;
    object      contactAngle;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            calculated;
        value           uniform -70;
    }
    right
    {
        type            calculated;
        value           uniform -70;
    }
    frontAndBack
    {
        type            empty;
    }
}

