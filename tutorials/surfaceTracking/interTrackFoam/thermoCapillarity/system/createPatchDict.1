FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

matchTolerance 1e-12;

pointSync true;

patchInfo
(
    {
        name             trackedSurface;
        constructFrom    patches;
        patches          (inner);

        dictionary
        {
            type         patch;
        }
    }

    {
        name             left;
        constructFrom    set;
        set              left;

        dictionary
        {
            type         patch;
        }
    }

    {
        name             right;
        constructFrom    set;
        set              right;

        dictionary
        {
            type         patch;
        }
    }

    {
        name             top;
        constructFrom    set;
        set              top;

        dictionary
        {
            type         patch;
        }
    }

    {
        name             bottom;
        constructFrom    set;
        set              bottom;

        dictionary
        {
            type         patch;
        }
    }

    {
        name             front;
        constructFrom    set;
        set              front;

        dictionary
        {
            type         empty;
        }
    }

    {
        name             back;
        constructFrom    set;
        set              back;

        dictionary
        {
            type         empty;
        }
    }
);

// OpenFOAM syntax
patches
(
);

