FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      VRe;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            empty;
    }
    conductorSurface
    {
        type            fixedGradient;
        gradient        uniform 0;
        value           $internalField;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}

