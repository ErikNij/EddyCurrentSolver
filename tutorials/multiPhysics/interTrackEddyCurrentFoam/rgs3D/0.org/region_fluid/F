FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      F;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    trackedSurface
    {
        type            calculated;
        value           $internalField;
    }
    movingWall
    {
        type            calculated;
        value           $internalField;
    }
    fixedWall
    {
        type            calculated;
        value           $internalField;
    }
    "procBoundary.*"
    {
        type            processor;
        value           $internalField;
    }
}

